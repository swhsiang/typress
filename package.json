{
  "name": "typress",
  "version": "0.0.5",
  "description": "typescript start kit",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production ts-node src/index.ts",
    "dev": "NODE_ENV=development ts-node src/index.ts",
    "test": "NODE_ENV=test mocha -c -R spec --bail --require ts-node/register tests/*.ts ",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "jscs": "jscs --fix .",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swhsiang/typress.git"
  },
  "keywords": [
    "typescript",
    "express",
    "tslint",
    "jscs",
    "sequelize",
    "pm2"
  ],
  "author": "swh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swhsiang/typress/issues"
  },
  "homepage": "https://github.com/swhsiang/typress#readme",
  "dependencies": {
    "bluebird": "3.4.1",
    "express": "4.14.0",
    "lodash": "4.14.1",
    "sequelize": "3.23.6"
  },
  "devDependencies": {
    "should": "10.0.0",
    "typescript-require": "0.2.9"
  }
}
